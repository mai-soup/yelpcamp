<% layout("layouts/boilerplate") -%>
    <h1>All Campgrounds</h1>
    <a href="/campgrounds/new">Add new</a>
    <ul>
        <% for (let camp of camps) { %>
            <li>
                <a href="/campgrounds/<%= camp._id %>">
                    <%= camp.title %>
                </a>
            </li>
            <% } %>
    </ul>